import("../../lib/aoc.vil");

fn main(args) {
  var total, lines = fs.get_lines("./data");

  each (var v : lines) {
    if (!#v) skip;

    var dims = str.spl(v, "x");

    var l = $dims[0],
        w = $dims[1],
        h = $dims[2];

    total += l * w * h;
  }

  io.println(total);
}

